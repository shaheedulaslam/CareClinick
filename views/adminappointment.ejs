 <!-- Spinner Start -->
<div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
    <div class="spinner-grow text-primary" style="width: 3rem; height: 3rem;" role="status">
        <span class="sr-only">Loading...</span>
    </div>
</div>
<!-- Spinner End -->

 <%- include('./partials/headeradmin.ejs') %>
 
 <!-- /Page Header -->
 <div class="row">
    <div class="col-sm-12">
        <div class="card">
            
            <div class="card-body">
                <h4 class="text-primary text-center">Appointments Lists</h4>
                <div class="table-responsive">
                    
                    <br>
                    <table class="datatable table table-hover table-center mb-0">
                        <thead>
                            <tr>
                                <th>Doctor Name</th>
                                <th>Speciality</th>
                                <th>Patients Name</th>
                                <th>Age</th>
                                <th>Appointments Date</th>
                                <th>Appointments Status</th>
                            </tr>
                        </thead>
                        <tbody> 
                         
                            <%adminappointlist.forEach(el=>{%>
                            <tr>
                                <td>
                                    <h5 class="fs-6"><%=docname.name%></h5> 
                                </td>
                                <td><%=el.doctor %></td>
                                <td><h5 class="fs-6"><%=el.name %></h5></td>
                                <td><%=el.Age %></td>
                                <td><small><%=el.date.toLocaleString()%></small></td>
                                <td><p class="text-success">Confirmed</p></td>
                            </tr>
                            <% }) %> 
                        </tbody>
                    </table>
                    
                   
                </div>
            </div>
           
        </div>
    </div>
</div>

</div>
</div>
<!-- /Page Wrapper -->